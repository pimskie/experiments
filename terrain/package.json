{
  "name": "terrain",
  "version": "1.0.0",
  "description": "terrain-generator",
  "main": "main.js",
  "scripts": {
    "dev": "parcel ./src/index.html",
    "build": "set NODE_ENV=production&&npm run clear && parcel build ./src/index.html --public-url . --no-cache",
    "deploy": "npm run build && firebase deploy",
    "clear": "rimraf ./dist && rimraf ./.cache",
    "start": "npm run clear && npm run dev"
  },
  "keywords": [
    "terrain",
    "js"
  ],
  "author": "pimskie",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "parcel-bundler": "^1.11.0",
    "sass": "^1.17.0"
  },
  "dependencies": {
    "dat.gui": "^0.7.5",
    "simplex-noise": "^2.4.0",
    "three": "^0.101.1",
    "three-orbitcontrols": "^2.101.1",
    "uikit": "^3.0.3"
  }
}
